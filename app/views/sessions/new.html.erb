<p id="notice"><%= notice %></p>
<p id="alert"><%= alert %></p>


<%= form_for :sessions, url:login_path do |f| %>

  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>

  <div class="actions">
    <%= f.submit "Log In" %>
  </div>

<% end %>
